{
  "kind": "collectionType",
  "collectionName": "categorias",
  "info": {
    "singularName": "categoria",
    "pluralName": "categorias",
    "displayName": "Categoria"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string"
    },
    "ID_Nombre": {
      "type": "uid",
      "targetField": "nombre"
    },
    "ID_Categoria": {
      "type": "string",
      "unique": false
    },
    "hijo_id": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::categoria.categoria",
      "mappedBy": "padre_id"
    },
    "padre_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::categoria.categoria",
      "inversedBy": "hijo_id"
    },
    "productos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::producto.producto",
      "mappedBy": "categoria"
    },
    "cat_img": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
